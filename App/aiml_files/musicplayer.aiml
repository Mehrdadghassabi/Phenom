<aiml version="2.0" encoding="UTF-8">


    <category>
        <pattern>START MUSIC PLAYER</pattern>
<template>
<think><set name="language">english</set></think>
            <think><system>bash -c 'xterm -hold -e "mocp"&amp;> /dev/null &amp;'</system></think>
            Yes sir
        </template>
    </category>

    <category>
        <pattern>RUN MUSIC PLAYER SERVER</pattern>
<template>
<think><set name="language">english</set></think>
            <think><system>mocp -S</system></think>
            Yes sir
        </template>
    </category>

